{
  "name": "Pluralize.name",
  "version": "0.3.14",
  "description": "Pluralize an English-language family name.",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test",
    "test": "NODE_ENV=production mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:cov": "babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "check": "npm run lint && npm run test  && flow check ",
    "build": "webpack src/index.js --config webpack.config.production.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nematode/pluralize.name.git"
  },
  "keywords": [
    "pluralize",
    "family name",
    "pluralizer"
  ],
  "author": "Trevor Farlow <npm@trevdiggy.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nematode/pluralize.name/issues"
  },
  "homepage": "https://github.com/nematode/pluralize.name#readme",
  "dependencies": {
    "extract-text-webpack-plugin": "^0.9.1",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.4.1",
    "express": "^4.13.3",
    "history": "^1.9.1",
    "istanbul": "^0.4.1",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "mocha-jsdom": "^1.0.0",
    "path": "^0.12.7",
    "proxyquire": "^1.7.2",
    "react-addons-test-utils": "^0.14.0-rc1",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.0",
    "webpack-hot-middleware": "^2.0.0"
  }
}
