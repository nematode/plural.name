{
  "name": "Pluralize.name",
  "version": "0.4.13",
  "description": "Pluralize an English-language family name.",
  "scripts": {
    "start": "node server.js",
    "clean": "rimraf lib dist",
    "lint": "eslint src test",
    "test": "NODE_ENV=production mocha --recursive",
    "test:watch": "npm test -- --watch",
    "check": "npm run lint && npm run test",
    "build": "webpack src/index.js --config webpack.config.production.js",
    "deploy": "scp dist/* plurl@plural.name:~/plural.name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevdor/pluralize.name.git"
  },
  "keywords": [
    "pluralize",
    "family name",
    "pluralizer"
  ],
  "author": "Trevor Farlow <npm@trevdor.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nematode/pluralize.name/issues"
  },
  "homepage": "https://github.com/nematode/pluralize.name#readme",
  "dependencies": {
    "milligram": "1.3.0",
    "react": "16.6.3",
    "react-dom": "16.6.3"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.1",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "mocha-loader": "^0.7.1",
    "node-sass": "^3.4.2",
    "path": "0.12.7",
    "proxyquire": "^1.7.2",
    "prop-types": "15.6.2",
    "react-addons-test-utils": "^15.0.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-validator": "^2.2.0"
  }
}
